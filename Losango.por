programa {
  funcao inicio() {
    inteiro diagonal_maior 
    inteiro diagonal_menor
    inteiro losango

    escreva("Diagonal maior do losango= \n")
    leia(diagonal_maior)
    escreva("Diagonal menor do losango= \n")
    leia(diagonal_menor)

    losango = diagonal_maior*diagonal_menor/2
    escreva("area= ", losango)
  }
}
