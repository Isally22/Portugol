programa {

  inclua biblioteca Util 
  
  funcao inicio() {
// Escreva um algoritmo para imprimir os 10 primeiros números inteiros maiores que 100.   

    inteiro i

      para(i = 101; i <= 110; i++)
      {
         escreva(i, "\n")
         Util.aguarde(700)
      }
     
     escreva("Essa foi a sequência dos dez primeiros números inteiros!")


  }
}
